{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "@next/next/no-html-link-for-pages": "error",
    "react/no-unescaped-entities": "warn",
    "prefer-const": "error",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ]
  }
}
